      <controls clazz="SP800-53-enhancement" id="cm-5.1" title="Automated Access Enforcement and Audit Records">
        <params id="cm-5.1_prm_1" label="organization-defined automated mechanisms"/>
        <props uuid="4cad7e18-3906-4062-93cc-5fc8526db7ea" value="CM-5(1)" name="label"/>
        <props uuid="7e69c193-65f6-4b2a-9d90-74d86ed31bf6" value="cm-05.01" name="sort-id"/>
        <links eHref="#cm-5" rel="required"/>
        <links eHref="#au-2" rel="related"/>
        <links eHref="#au-6" rel="related"/>
        <links eHref="#au-7" rel="related"/>
        <links eHref="#au-12" rel="related"/>
        <links eHref="#cm-6" rel="related"/>
        <links eHref="#cm-11" rel="related"/>
        <links eHref="#si-12" rel="related"/>
        <parts id="cm-5.1_smt" name="statement">
          <parts id="cm-5.1_smt.a" name="item" prose="Enforce access restrictions using {{ insert: param, cm-5.1_prm_1 }}; and">
            <props uuid="13d459f5-19cd-4aea-9ea0-049b21bfecf8" value="(a)" name="label"/>
          </parts>
          <parts id="cm-5.1_smt.b" name="item" prose="Automatically generate audit records of the enforcement actions.">
            <props uuid="8caa55c3-bb55-443e-937b-bbc2841ae9f6" value="(b)" name="label"/>
          </parts>
        </parts>
        <parts id="cm-5.1_gdn" name="guidance" prose="Organizations log system accesses associated with applying configuration changes to ensure that configuration change control is implemented and to support after-the-fact actions should organizations discover any unauthorized changes."/>
      </controls>
      <controls clazz="SP800-53-enhancement" id="cm-5.2" title="Review System Changes">
        <props uuid="597f1f48-d62a-4d74-afc1-9ccbd578e059" value="CM-5(2)" name="label"/>
