      <props uuid="f7b129e7-5816-412a-b5c3-0610053a6144" value="PL-8" name="label"/>
      <props uuid="0f088f7e-4f1e-447e-834f-39aac9bb3e70" value="pl-08" name="sort-id"/>
      <links eHref="#27847491-5ce1-4f6a-a1e4-9e483782f0ef" rel="reference"/>
