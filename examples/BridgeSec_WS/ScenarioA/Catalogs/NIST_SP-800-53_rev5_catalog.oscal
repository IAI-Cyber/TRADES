        <links eHref="#ir-9" rel="incorporated-into"/>
      </controls>
      <controls clazz="SP800-53-enhancement" id="ir-9.2" title="Training">
        <params id="ir-9.2_prm_1" label="organization-defined frequency"/>
        <props uuid="148a1490-0df7-4526-b2ef-d183e7daefc5" value="IR-9(2)" name="label"/>
        <props uuid="c9a13624-eff9-4f6f-8191-8cceda98696c" value="ir-09.02" name="sort-id"/>
        <links eHref="#ir-9" rel="required"/>
