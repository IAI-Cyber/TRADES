      <controls clazz="SP800-53-enhancement" id="sc-12.5" title="PKI Certificates / Hardware Tokens">
        <props uuid="3243500d-b9b6-4b0c-9656-51d130e86781" value="SC-12(5)" name="label"/>
        <props uuid="a9fd5e4d-532e-49dc-8a57-ad4698cf3063" value="sc-12.05" name="sort-id"/>
