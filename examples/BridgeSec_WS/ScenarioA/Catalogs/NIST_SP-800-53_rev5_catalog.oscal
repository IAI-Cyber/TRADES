      <controls clazz="SP800-53-enhancement" id="sa-19.2" title="Configuration Control for Component Service and Repair">
        <props uuid="6dde259c-d38c-4a0c-a8de-c5d328fb6c63" value="SA-19(2)" name="label"/>
