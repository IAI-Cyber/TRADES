      <params id="pe-1_prm_4" label="organization-defined frequency"/>
      <params id="pe-1_prm_5" label="organization-defined events"/>
      <params id="pe-1_prm_6" label="organization-defined frequency"/>
      <params id="pe-1_prm_7" label="organization-defined events"/>
      <props uuid="fa5c29dd-c501-42b3-a62e-a6402bf80180" value="PE-1" name="label"/>
      <props uuid="2c1479d6-f676-49fd-abb8-ac216d70979d" value="pe-01" name="sort-id"/>
      <links eHref="#c7ac44e8-10db-4b64-b2b9-9e32ec1efed0" rel="reference"/>
      <links eHref="#08b07465-dbdc-48d6-8a0b-37279602ac16" rel="reference"/>
      <links eHref="#cec037f3-8aba-4c97-84b4-4082f9e515d2" rel="reference"/>
      <links eHref="#4c0ec2ee-a0d6-428a-9043-4504bc3ade6f" rel="reference"/>
