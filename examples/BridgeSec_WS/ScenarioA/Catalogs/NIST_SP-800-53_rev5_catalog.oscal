      <controls clazz="SP800-53-enhancement" id="cm-2.1" title="Reviews and Updates">
        <props uuid="c9121d26-bb2b-46f8-8a2f-175ffe595a10" value="CM-2(1)" name="label"/>
        <props uuid="83b3aa28-9be6-4724-90f0-32e9b4b6fb9e" value="cm-02.01" name="sort-id"/>
