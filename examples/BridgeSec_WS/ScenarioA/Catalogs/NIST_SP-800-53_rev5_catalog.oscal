      <controls clazz="SP800-53-enhancement" id="mp-5.3" title="Custodians">
        <props uuid="03bc7670-21e2-40e1-b8c0-a5d89b6b0ff1" value="MP-5(3)" name="label"/>
        <props uuid="958a9c08-eeb9-4516-aae2-480fd447fa81" value="mp-05.03" name="sort-id"/>
        <links eHref="#mp-5" rel="required"/>
        <parts id="mp-5.3_smt" name="statement" prose="Employ an identified custodian during transport of system media outside of controlled areas."/>
        <parts id="mp-5.3_gdn" name="guidance" prose="Identified custodians provide organizations with specific points of contact during the media transport process and facilitate individual accountability. Custodial responsibilities can be transferred from one individual to another if an unambiguous custodian is identified."/>
      </controls>
      <controls clazz="SP800-53-enhancement" id="mp-5.4" title="Cryptographic Protection">
        <props uuid="d60bfc1d-d5e5-44f9-8e38-8329d5a50706" value="MP-5(4)" name="label"/>
        <props uuid="06caf5af-041d-4789-8408-7649907a6487" value="mp-05.04" name="sort-id"/>
