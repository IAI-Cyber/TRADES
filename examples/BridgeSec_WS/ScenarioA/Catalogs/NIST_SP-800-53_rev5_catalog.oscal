      <controls clazz="SP800-53-enhancement" id="mp-4.2" title="Automated Restricted Access">
        <params id="mp-4.2_prm_1" label="organization-defined automated mechanisms"/>
        <props uuid="dc9ebb10-84ae-4d51-af80-5bca6e916302" value="MP-4(2)" name="label"/>
        <props uuid="a92036c8-169b-4cdf-a67d-d40f3f134045" value="mp-04.02" name="sort-id"/>
        <links eHref="#mp-4" rel="required"/>
        <links eHref="#ac-3" rel="related"/>
        <links eHref="#au-2" rel="related"/>
        <links eHref="#au-6" rel="related"/>
        <links eHref="#au-9" rel="related"/>
        <links eHref="#au-12" rel="related"/>
        <links eHref="#pe-3" rel="related"/>
        <parts id="mp-4.2_smt" name="statement" prose="Restrict access to media storage areas and log access attempts and access granted using {{ insert: param, mp-4.2_prm_1 }}."/>
        <parts id="mp-4.2_gdn" name="guidance" prose="Automated mechanisms include keypads, biometric readers, or card readers on the external entries to media storage areas."/>
      </controls>
      <parts id="mp-4_smt" name="statement">
        <parts id="mp-4_smt.a" name="item" prose="Physically control and securely store {{ insert: param, mp-4_prm_1 }} within {{ insert: param, mp-4_prm_2 }}; and">
          <props uuid="77e44cd9-e7d2-460d-a7e6-346794e6b93d" value="a." name="label"/>
        </parts>
        <parts id="mp-4_smt.b" name="item" prose="Protect system media types defined in MP-4a until the media are destroyed or sanitized using approved equipment, techniques, and procedures.">
          <props uuid="0c6e62de-4922-4f3e-beb3-f12ae80d84d5" value="b." name="label"/>
        </parts>
      </parts>
      <parts id="mp-4_gdn" name="guidance" prose="System media includes digital and non-digital media. Digital media includes flash drives, diskettes, magnetic tapes, external or removable hard disk drives (e.g., solid state, magnetic), compact discs, and digital versatile discs. Non-digital media includes paper and microfilm. Physically controlling stored media includes conducting inventories, ensuring procedures are in place to allow individuals to check out and return media to the library, and maintaining accountability for stored media. Secure storage includes a locked drawer, desk, or cabinet or a controlled media library. The type of media storage is commensurate with the security category or classification of the information on the media. Controlled areas are spaces that provide physical and procedural controls to meet the requirements established for protecting information and systems. Fewer controls may be needed for media that contains information determined to be in the public domain, publicly releasable, or have limited adverse impacts on organizations, operations, or individuals if accessed by other than authorized personnel. In these situations, physical access controls provide adequate protection."/>
    </controls>
