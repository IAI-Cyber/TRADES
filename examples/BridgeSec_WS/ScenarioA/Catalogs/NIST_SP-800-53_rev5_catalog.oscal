        <parts id="pt-2.1_gdn" name="guidance" prose="Data tags support the tracking and enforcement of authorized processing by conveying the types of processing that are authorized along with the relevant elements of personally identifiable information throughout the system. Data tags may also support the use of automated tools."/>
      </controls>
      <controls clazz="SP800-53-enhancement" id="pt-2.2" title="Automation">
        <params id="pt-2.2_prm_1" label="organization-defined automated mechanisms"/>
