      <params id="ma-1_prm_4" label="organization-defined frequency"/>
      <params id="ma-1_prm_5" label="organization-defined events"/>
      <params id="ma-1_prm_6" label="organization-defined frequency"/>
      <params id="ma-1_prm_7" label="organization-defined events"/>
      <props uuid="30358b4d-12bc-4dcf-a27a-278b0fcbe02d" value="MA-1" name="label"/>
      <props uuid="45b30ef2-33b7-42b7-9b2e-41f5d0a9060e" value="ma-01" name="sort-id"/>
      <links eHref="#27847491-5ce1-4f6a-a1e4-9e483782f0ef" rel="reference"/>
      <links eHref="#c7ac44e8-10db-4b64-b2b9-9e32ec1efed0" rel="reference"/>
      <links eHref="#08b07465-dbdc-48d6-8a0b-37279602ac16" rel="reference"/>
      <links eHref="#cec037f3-8aba-4c97-84b4-4082f9e515d2" rel="reference"/>
