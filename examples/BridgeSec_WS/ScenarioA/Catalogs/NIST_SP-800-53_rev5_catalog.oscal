      <controls clazz="SP800-53-enhancement" id="ir-5.1" title="Automated Tracking, Data Collection, and Analysis">
        <params id="ir-5.1_prm_1" label="organization-defined automated mechanisms"/>
        <props uuid="578a9f51-15a4-4404-a0eb-104c3a1dd45e" value="IR-5(1)" name="label"/>
        <props uuid="de989c7f-6603-407b-a734-9b71dc4ef863" value="ir-05.01" name="sort-id"/>
        <links eHref="#ir-5" rel="required"/>
        <parts id="ir-5.1_smt" name="statement" prose="Track incidents and collect and analyze incident information using {{ insert: param, ir-5.1_prm_1 }}."/>
        <parts id="ir-5.1_gdn" name="guidance" prose="Automated mechanisms for tracking incidents and collecting and analyzing incident information include Computer Incident Response Centers or other electronic databases of incidents and network monitoring devices."/>
      </controls>
      <parts id="ir-5_smt" name="statement" prose="Track and document incidents."/>
      <parts id="ir-5_gdn" name="guidance" prose="Documenting incidents includes maintaining records about each incident, the status of the incident, and other pertinent information necessary for forensics as well as evaluating incident details, trends, and handling. Incident information can be obtained from a variety of sources, including network monitoring, incident reports, incident response teams, user complaints, supply chain partners, audit monitoring, physical access monitoring, and user and administrator reports. [IR-4](#ir-4) provides information on the types of incidents that are appropriate for monitoring."/>
    </controls>
    <controls clazz="SP800-53" id="ir-6" title="Incident Reporting">
      <params id="ir-6_prm_1" label="organization-defined time period"/>
      <params id="ir-6_prm_2" label="organization-defined authorities"/>
      <props uuid="f7811b89-2839-4e13-9a49-6b9b587ed8e4" value="IR-6" name="label"/>
      <props uuid="cf678cac-6b1a-4abc-b8d0-7fb6c5e71197" value="ir-06" name="sort-id"/>
      <links eHref="#4ff10ed3-d8fe-4246-99e3-443045e27482" rel="reference"/>
      <links eHref="#0f963c17-ab5a-432a-a867-91eac550309b" rel="reference"/>
      <links eHref="#40b78258-c892-480e-9af8-77ac36648301" rel="reference"/>
      <links eHref="#49b8aa2d-a88c-4bff-9f20-876ccb8f7dcb" rel="reference"/>
