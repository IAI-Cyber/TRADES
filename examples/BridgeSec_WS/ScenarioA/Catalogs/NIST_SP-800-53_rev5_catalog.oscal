      <links eHref="#at-3" rel="related"/>
      <links eHref="#ir-4" rel="related"/>
      <links eHref="#ir-6" rel="related"/>
      <links eHref="#ir-8" rel="related"/>
      <links eHref="#pm-22" rel="related"/>
      <links eHref="#pm-26" rel="related"/>
      <links eHref="#sa-9" rel="related"/>
      <links eHref="#si-18" rel="related"/>
      <controls clazz="SP800-53-enhancement" id="ir-7.1" title="Automation Support for Availability of Information and Support">
        <params id="ir-7.1_prm_1" label="organization-defined automated mechanisms"/>
        <props uuid="b9127b49-609c-4104-a303-84aaa3a40ba4" value="IR-7(1)" name="label"/>
        <props uuid="a13481ce-bac7-4021-8e8b-e6b6607ea288" value="ir-07.01" name="sort-id"/>
        <links eHref="#ir-7" rel="required"/>
        <parts id="ir-7.1_smt" name="statement" prose="Increase the availability of incident response information and support using {{ insert: param, ir-7.1_prm_1 }}."/>
        <parts id="ir-7.1_gdn" name="guidance" prose="Automated mechanisms can provide a push or pull capability for users to obtain incident response assistance. For example, individuals may have access to a website to query the assistance capability, or the assistance capability can proactively send incident response information to users (general distribution or targeted) as part of increasing understanding of current response capabilities and support."/>
      </controls>
      <controls clazz="SP800-53-enhancement" id="ir-7.2" title="Coordination with External Providers">
        <props uuid="46564e7e-e8fd-4cb6-b8fb-b661e9815832" value="IR-7(2)" name="label"/>
        <props uuid="1003df61-0eb4-48f4-8572-a136d416b0ac" value="ir-07.02" name="sort-id"/>
        <links eHref="#ir-7" rel="required"/>
        <parts id="ir-7.2_smt" name="statement">
          <parts id="ir-7.2_smt.a" name="item" prose="Establish a direct, cooperative relationship between its incident response capability and external providers of system protection capability; and">
            <props uuid="dbf0e808-7865-43d1-958b-f66e7ad74aae" value="(a)" name="label"/>
          </parts>
          <parts id="ir-7.2_smt.b" name="item" prose="Identify organizational incident response team members to the external providers.">
            <props uuid="c9bbbdca-864d-41fc-956e-35609ad5cb8e" value="(b)" name="label"/>
          </parts>
        </parts>
        <parts id="ir-7.2_gdn" name="guidance" prose="External providers of a system protection capability include the Computer Network Defense program within the U.S. Department of Defense. External providers help to protect, monitor, analyze, detect, and respond to unauthorized activity within organizational information systems and networks. It may be beneficial to have agreements in place with external providers to clarify the roles and responsibilities of each party before an incident occurs."/>
      </controls>
      <parts id="ir-7_smt" name="statement" prose="Provide an incident response support resource, integral to the organizational incident response capability, that offers advice and assistance to users of the system for the handling and reporting of incidents."/>
      <parts id="ir-7_gdn" name="guidance" prose="Incident response support resources provided by organizations include help desks, assistance groups, automated ticketing systems to open and track incident response tickets, and access to forensics services or consumer redress services, when required."/>
    </controls>
    <controls clazz="SP800-53" id="ir-8" title="Incident Response Plan">
      <params id="ir-8_prm_1" label="organization-defined personnel or roles"/>
      <params id="ir-8_prm_2" label="organization-defined frequency"/>
      <params id="ir-8_prm_3" label="organization-defined entities, personnel, or roles"/>
      <params id="ir-8_prm_4" label="organization-defined incident response personnel (identified by name and/or by role) and organizational elements"/>
      <params id="ir-8_prm_5" label="organization-defined incident response personnel (identified by name and/or by role) and organizational elements"/>
      <props uuid="4d85ddc0-626e-4ad0-a82a-bd823a830251" value="IR-8" name="label"/>
      <props uuid="21c7ebef-df08-485e-8669-062bc717d940" value="ir-08" name="sort-id"/>
      <links eHref="#27847491-5ce1-4f6a-a1e4-9e483782f0ef" rel="reference"/>
