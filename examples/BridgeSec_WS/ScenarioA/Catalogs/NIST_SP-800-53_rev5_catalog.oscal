        <props uuid="ba6fad07-90ea-4c56-815a-ac276641a09f" value="withdrawn" name="status"/>
        <links eHref="#mp-6" rel="incorporated-into"/>
      </controls>
      <controls clazz="SP800-53-enhancement" id="mp-6.5" title="Classified Information">
        <props uuid="b7cd9eab-13c2-4ee3-bed9-92a19c52690c" value="MP-6(5)" name="label"/>
        <props uuid="a22df42f-ac7c-4d0f-acaa-e5a4caccd562" value="mp-06.05" name="sort-id"/>
        <props uuid="348c9edc-c2cd-44da-b1a3-ac08cc8236f0" value="withdrawn" name="status"/>
        <links eHref="#mp-6" rel="incorporated-into"/>
