          <choice>System-level</choice>
        </select>
      </params>
      <params id="sa-1_prm_3" label="organization-defined official"/>
      <params id="sa-1_prm_4" label="organization-defined frequency"/>
      <params id="sa-1_prm_5" label="organization-defined events"/>
      <params id="sa-1_prm_6" label="organization-defined frequency"/>
      <params id="sa-1_prm_7" label="organization-defined events"/>
      <props uuid="8ddccf88-6212-47f0-ac99-e5938fc66b99" value="SA-1" name="label"/>
      <props uuid="faceee62-3e62-4af1-966d-939004e58d16" value="sa-01" name="sort-id"/>
      <links eHref="#27847491-5ce1-4f6a-a1e4-9e483782f0ef" rel="reference"/>
      <links eHref="#c7ac44e8-10db-4b64-b2b9-9e32ec1efed0" rel="reference"/>
      <links eHref="#08b07465-dbdc-48d6-8a0b-37279602ac16" rel="reference"/>
      <links eHref="#cec037f3-8aba-4c97-84b4-4082f9e515d2" rel="reference"/>
      <links eHref="#4c0ec2ee-a0d6-428a-9043-4504bc3ade6f" rel="reference"/>
      <links eHref="#e3cc0520-a366-4fc9-abc2-5272db7e3564" rel="reference"/>
