      <controls clazz="SP800-53-enhancement" id="ra-5.1" title="Update Tool Capability">
        <props uuid="45661c66-2263-49d5-ac8b-9c1f1c84085f" value="RA-5(1)" name="label"/>
        <props uuid="b6558dc0-93a1-4eae-afbb-a539a2f0070b" value="ra-05.01" name="sort-id"/>
        <props uuid="43feffed-1baa-4d4d-9916-bd497cb4bcaf" value="withdrawn" name="status"/>
