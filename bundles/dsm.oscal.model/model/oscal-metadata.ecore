        <details key="source" value="&lt;Metaschema>oscal-metadata.&lt;AssemblyDefinition>revision"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="roles" upperBound="-1" eType="//Role" containment="true">
      <eAnnotations source="Metaschema">
        <details key="source" value="&lt;Metaschema>oscal-metadata.&lt;AssemblyDefinition>role"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="title" lowerBound="1" eType="//MarkupLineType">
      <eAnnotations source="Metaschema">
        <details key="source" value="&lt;Metaschema>oscal-metadata.&lt;AssemblyDefinition>metadata.&lt;FieldInstance>title"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="&lt;h1>Document Title&lt;/h1>&#xA;&lt;h2>Description&lt;/h2>&#xA;&lt;p>A name given to the document, which may be used by a tool for display and navigation.&lt;/p>&#xA;"/>
