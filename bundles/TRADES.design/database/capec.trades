    <externals source="Capec" sourceID="Capec" name="Validate all untrusted data." id="Control_51798d96120b936709c9b9dc330cc4379287fc33d2ed50f01cfec41a78b8ad0f">
      <mitigationRelations threat="//@threatOwner/@externals.486"/>
