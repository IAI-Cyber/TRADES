    <externals source="Capec" sourceID="Capec" name="Set the OPTIONS IncludesNOEXEC in the global access.conf file or local .htaccess (Apache) file to deny SSI execution in directories that do not need them" id="Control_23313faa1a11d0cd14b67a7b7908e1716842d1018cefd3b928750b6ed39dcd8b">
