    <externals source="Capec" sourceID="Capec" name="When maintaining an authenticated session with a privileged target system, do not use the same browser to navigate to unfamiliar sites to perform other activities. Finish working with the target system and logout first before proceeding to other tasks." id="Control_b456e2f3d43fd1fce1dcf7fbc9286d0de2f2c6d1215e7699812a80ea9363051a">
      <mitigationRelations threat="//@threatOwner/@externals.5"/>
    </externals>
