    </externals>
    <externals source="Capec" sourceID="Capec" name="Encryption of all data packets emanating from the smartphone to a retransmission device via two encrypted tunnels with Suite B cryptography, all the way to the VPN gateway at the datacenter." id="Control_cf79c41eab27d7ec2e3b490af35931d8bcadd4d2be86cf3425604212253bbd74">
      <mitigationRelations threat="//@threatOwner/@externals.435"/>
    </externals>
    <externals source="Capec" sourceID="Capec" name="Use a strict session management mechanism that only accepts locally generated session identifiers: This prevents attackers from fixating session identifiers of their own choice." id="Control_2e4903821172d13893fefbc43dad74ff9f9129db60321727c65db90600f4ba74">
