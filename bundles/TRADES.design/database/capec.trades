    <externals source="Capec" sourceID="Capec" name="Do not rely solely on user-controllable locations, such as cookies or URL parameters, to maintain user state." id="Control_b9b90d6e4dc72fabdc196275294b7e3818a69622b75d7b9c9f45177bc74c81bc">
