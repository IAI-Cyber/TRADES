    <externals source="Capec" sourceID="Capec" name="Verify that the user-supplied data does not use backslash character to escape malicious characters." id="Control_f5a5e58258c98432bf972afa95321d102ed9a98a8dbb776c849f9786f44bdd6b">
      <mitigationRelations threat="//@threatOwner/@externals.496"/>
