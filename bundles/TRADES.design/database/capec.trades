    <externals source="Capec" sourceID="Capec" name="Get your Public Key signed by a Certificate Authority" id="Control_2e6a45bad73e408c8cb7f354033638d2c1a97574eb5c82ab7a795aa0c816dacd">
      <mitigationRelations threat="//@threatOwner/@externals.512"/>
    </externals>
    <externals source="Capec" sourceID="Capec" name="Encrypt your communication using cryptography (SSL,...)" id="Control_e2aeb49df03747b252e5ad4bf20727d0932bb6e4bfc0e36b11ed7df9e7defeb6">
