    <externals source="Capec" sourceID="Capec" name="Server Side Includes must be enabled only if there is a strong business reason to do so. Every additional component enabled on the web server increases the attack surface as well as administrative overhead" id="Control_5c2218bc82cb68bea0277b4a1d803bad69038e2ef0fb1a52a85c88e33ddb38ca">
      <mitigationRelations threat="//@threatOwner/@externals.3"/>
    </externals>
    <externals source="Capec" sourceID="Capec" name="Make sure that HTTPS is used to communicate with the target system. Alternatively, use VPN if possible. It is important to ensure that all communication between the client and the server happens via an encrypted secure channel." id="Control_29578b94dc9b375d6486d917a85dff58ddd7e229cb7cb49a96e1230737ef3b8c">
