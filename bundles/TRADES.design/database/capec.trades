    <externals source="Capec" sourceID="Capec" name="Disable MSSQL xp_cmdshell directive on the database" id="Control_28816fed5aa01fd0598396e5d6b2cba491571cc28c6aa6f25d0017f991a31a3d">
      <mitigationRelations threat="//@threatOwner/@externals.9"/>
    </externals>
    <externals source="Capec" sourceID="Capec" name="Properly validate the data (syntactically and semantically) before writing it to the database." id="Control_734a7a96cb4e33e95b63b3a483f85078964836f2246f1c3b3728468891cf4db2">
