   <xs:group name="blockElementGroup">
      <xs:sequence>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="h1" type="oscal-ap:blockElementType"/>
            <xs:element name="h2" type="oscal-ap:blockElementType"/>
            <xs:element name="h3" type="oscal-ap:blockElementType"/>
            <xs:element name="h4" type="oscal-ap:blockElementType"/>
            <xs:element name="h5" type="oscal-ap:blockElementType"/>
            <xs:element name="h6" type="oscal-ap:blockElementType"/>
